{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
  "manifestVersion": "1.16",
  "version": "1.0.0",
  "id": "${{TEAMS_APP_ID}}",
  "packageName": "com.microsoft.teams.extension",
  "developer": {
    "name": "Advanced Software Technology Pro",
    "websiteUrl": "https://www.example.com",
    "privacyUrl": "https://www.example.com/privacy",
    "termsOfUseUrl": "https://www.example.com/termofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "ELTE_chatbot${{APP_NAME_SUFFIX}}",
    "full": "full name for ELTE_chatbot"
  },
  "description": {
    "short": "Description of the bot",
    "full": "Interacting with the AI chatbot within Microsoft Teams is like having a helpful virtual assistant right in your messaging platform. It swiftly responds to your inquiries, offers relevant information efficiently. With its intuitive understanding of natural language, the chatbot enhances your Teams experience by providing instant support and guidance whenever you need it."
  },
  "accentColor": "#FFFFFF",
  "bots": [
    {
      "botId": "${{BOT_ID}}",
      "scopes": ["personal", "team", "groupchat"],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "composeExtensions": [],
  "configurableTabs": [],
  "staticTabs": [],
  "permissions": ["identity", "messageTeamMembers"],
  "validDomains": []
}
